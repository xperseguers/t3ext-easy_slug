{
  "name": "causal/easy_slug",
  "type": "typo3-cms-extension",
  "description": "Updates the slug of a page automatically and logically when renaming a page or moving it around.",
  "keywords": [
    "TYPO3 CMS",
    "slug",
    "pages",
    "url",
    "uri",
    "update"
  ],
  "homepage": "https://github.com/xperseguers/t3ext-easy_slug",
  "support": {
    "issues": "https://github.com/xperseguers/t3ext-easy_slug/issues"
  },
  "authors": [
    {
      "name": "Xavier Perseguers",
      "email": "xavier@causal.ch",
      "homepage": "https://www.causal.ch",
      "role": "Developer"
    }
  ],
  "license": [
    "GPL-2.0-or-later",
    "GPL-3.0-or-later"
  ],
  "require": {
    "php": ">=7.4.0 <=7.4.99 || >=8.0.0 <=8.2.99",
    "typo3/cms-core": "^10 || ^11 || ^12",
    "typo3/cms-redirects": "^10 || ^11 || ^12"
  },
  "conflict": {
    "hendrikreimers/slug_autoupdate": "*"
  },
  "autoload": {
    "psr-4": {
      "Causal\\EasySlug\\": "Classes/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    },
    "typo3/cms": {
      "extension-key": "easy_slug"
    }
  }
}
